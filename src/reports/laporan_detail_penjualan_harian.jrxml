	<queryString>
		<![CDATA[select 
 transactions.date, 
 transactions.total, 
 transactions.transaction_code,
 med.name,
 med.base_price,
 med.price,
 (med.price - med.base_price) as profit, 
 transaction_details.qty 
from transactions
join transaction_details
 on transaction_details.transaction_id = transactions.id
join med
 on med.id = transaction_details.med_id
where
 date = $P{date}]]>
	</queryString>
